<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	<PreferenceCategory android:title="General Settings"
		android:key="general">
		<PreferenceScreen android:key="customization" android:title="@string/pt_customization"
			android:summary="@string/ps_customization">
			<intent android:action="android.intent.action.MAIN"
				android:targetPackage="com.liberty.settings" android:targetClass="com.liberty.settings.Customizer" />
		</PreferenceScreen>
		<PreferenceScreen android:key="performance" android:title="@string/pt_performance"
			android:summary="@string/ps_performance">
			<intent android:action="android.intent.action.MAIN"
				android:targetPackage="com.liberty.settings" android:targetClass="com.liberty.settings.Performance" />
		</PreferenceScreen>
		<PreferenceScreen android:key="dsp_manager" android:title="DSP Manager"
			android:summary="A stand alone equalizer">
			<intent android:action="android.intent.action.MAIN"
				android:targetPackage="com.bel.android.dspmanager" android:targetClass="com.bel.android.dspmanager.activity.DSPManager" />
		</PreferenceScreen>
	</PreferenceCategory>
</PreferenceScreen>